<!DOCTYPE html>
<html>
<head>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<link rel="stylesheet" href="style.css">
</head>
<body>

<div class="topnav" style="margin-bottom: 0px; padding: 0px;">
  <a class="active" href="/">Home</a>
  <div class="search-container">
    <form action="/search" method="post">
      <input style="height: 34px;" type="text" placeholder="Search.." name="search">
      <button type="submit"><i class="fa fa-search"></i></button>
    </form>
  </div>
</div>

<div style="padding-left:16px">
    <h2>Pharmacy</h2>
    <div class="wrapper">

        <table id="customers">
            <tr>
                <th>Id pharmaceutical</th>
                <th>Pharmaceutical name</th>
                <th>Pharmaceutical type</th>
                <th>Quantity</th>
                <th>Price</th>
                <th>Pharmacy location</th>
                
            </tr>
                        
       
        
            <% for(let i = 0; i< drugs.length; i++) {%>

                
                <tr>
                    <td><%= drugs[i].idpharmaceutical %></td>
                    <td><%= drugs[i].pharmaceuticalname %></td>
                    <td><%= drugs[i].pharmaceuticaltype %></td>
                    <td><%= drugs[i].quantity %></td>
                    <td><%= drugs[i].price %></td>
                    <td><%= drugs[i].pharmacylocation %></td>
                    
                </tr>
                
                
            <% } %>

        </table>
        
   </div>
   
        


</body>
</html>
